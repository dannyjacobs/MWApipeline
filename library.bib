month = {Sep},
keywords = {X-ray, Background radiations, Mass and mass distribution, Observational cosmology}, 
date-added = {2010-07-10 18:32:19 -0700},
date-modified = {2010-07-10 18:32:19 -0700},
doi = {10.1103/PhysRevD.74.083517},
pmid = {2006PhRvD..74h3517S},
