@article{Dickey:1999p1209,
author = {J. M Dickey and M Marx-Zimmer and C D{\"u}sterberg and U Meb and S Stanimirovic and L Staveley-Smith and H. A Kobulnicky}, 
journal = {New Views of the Magellanic Clouds},
title = {Interstellar Phases in the Magellanic Clouds},
abstract = {In the seven years since the Australia Telescope Compact Array began spectral line observations, there have been six surveys of lambda21-cm absorption in and around the Magellanic Clouds. These observations tell us about the mixture of warm and cool phases in the atomic gas, about the abundance of diffuse clouds, and about the heating and cooling balance in the interstellar environment of the Magellanic Clouds. The primary result is that the cool atomic phase of the gas is quite abundant. Comparing the integral of the emission spectrum (proportional to the column density of H I) to the integral of the absorption spectrum (equivalent width) gives the mean spin temperature, which measures the relative abundance of warm and cool phases in the atomic medium. The Magellanic Clouds show numbers for the mean spin temperature which are lower than for the Milky Way, suggesting that roughly twice as much of the atomic medium is in the cool, diffuse cloud phase relative to warm, "intercloud" H I. Another interpretation of our results is that the temperature of the gas in typical atomic clouds is lower than in Milky Way diffuse clouds by a factor of two. This is confirmed in a few directions near 30 Dor by profile analysis which allows us to separate the warm and cool phase contributions to the spectra. The typical temperatures for the cool H I in the Clouds may be in the range 25 to 45 K, vs. 50 to 70 K in the solar neighborhood. This fits with the theoretical prediction for heating-cooling balance in a medium with metallicity and dust to gas ratio much less than their "cosmic" values. This result is particularly interesting in the context of the relatively low abundance of CO in the Clouds, especially in the SMC. Either the X factor for conversion of CO intensity to H2 column density is lower than its Milky Way value, and/or the transition between atomic and molecular hydrogen takes place deeper in the cores of dense clouds. This raises the question of whether gravitational collapse and star formation can proceed directly from dense, cold atomic clouds without passing through the molecular cloud stage. Another problem is raised by the discovery of several cool atomic clouds well outside the stellar boundaries of the LMC and SMC, in the H I envelope around the Clouds, and particularly in the Bridge region between them. The pressure in these regions must be surprisingly high for cool phase gas to exist.},
note = {ISBN: 1-58381-021-8},
pages = {45},
volume = {190},
year = {1999},
month = {Jan},
date-added = {2008-03-23 19:26:58 -0700},
